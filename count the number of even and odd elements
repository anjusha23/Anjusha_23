Question:C program to count the number of even and odd elements in an array 
Code:
#include<iostream>
using namespace std;
int main()
{
    int a;
    int c2=0;
    int c1=0;
    cin>>a;
    int m[a];
    for(int i=0;i<a;i++)
    {
        cin>>m[i];
    }
    for(int i=0;i<a;i++)
    {
        if(m[i]%2==0)
        {
        
        c2=c2+1;
        }
        else
        {
        
        c1=c1+1;
        }
    }
    cout<<"Odd:"<<c1<<endl;
    cout<<"Even:"<<c2;
}
