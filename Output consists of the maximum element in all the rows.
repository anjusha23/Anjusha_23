Question:Output consists of the maximum element in all the rows.
Code:
#include<iostream>
using namespace std;
int main()
{
    int r,c;
    cin>>r>>c;
    int m[r][c],n[r];
    for(int i=0;i<r;i++)
    {
        for(int j=0;j<c;j++)
        {
            cin>>m[i][j];
        }
    }
    for(int i=0;i<r;i++)
    {
        for(int j=0;j<c;j++)
        {
            if(m[i][0]<m[i][j]) 
            {
                m[i][0]=m[i][j];
            }
            n[r]=m[i][0];
        }
    cout<<n[r]<<endl;   
    }
    
}
